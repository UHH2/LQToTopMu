<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE JobConfiguration PUBLIC "" "JobConfig.dtd"[


	  <!ENTITY TargetLumi      "24920">
	  <!ENTITY Trigger         "Iso">
	  <!ENTITY SELdir          "/nfs/dust/cms/user/reimersa/LQToTopMu/Run2_92X_v1/EarlyData/Optimization/&TargetLumi;fb_GoldenJSON_&Trigger;">
	  <!ENTITY b_Cacheable     "False">
	  <!ENTITY NEVT            "-1">
	  <!ENTITY PRESELdir       "/nfs/dust/cms/user/reimersa/LQToTopMu/Run2_92X_v1/EarlyData/Preselection/&TargetLumi;fb_NoJSON/Iso"> 
	  <!ENTITY PRESELfilename  "uhh2.AnalysisModuleRunner"> 
	  <!ENTITY PROOFdir        "/nfs/dust/cms/user/reimersa/.proof2">





<!ENTITY DATA_B   "&PRESELdir;/&PRESELfilename;.DATA.DATA_B.root">
<!ENTITY DATA_C   "&PRESELdir;/&PRESELfilename;.DATA.DATA_C.root">
<!ENTITY DATA_D   "&PRESELdir;/&PRESELfilename;.DATA.DATA_D.root">
<!ENTITY DATA_E   "&PRESELdir;/&PRESELfilename;.DATA.DATA_E.root">
<!ENTITY DATA_F   "&PRESELdir;/&PRESELfilename;.DATA.DATA_F.root">

]>



<!--
<ConfigParse NEventsBreak="100000" FileSplit="0" AutoResubmit="4"/>
<ConfigSGE RAM ="4" DISK ="3" Mail="arne.reimers@desy.de" Notification="as" Workdir="workdir_EarlyData"/>
-->

<JobConfiguration JobName="ExampleCycleJob" OutputLevel="INFO">
  <Library Name="libSUHH2LQToTopMu"/>
  <Package Name="SUHH2LQToTopMu.par" />

  <!--
     **************************** 
     *                          *
     *       All Samples        *
     *                          *
     ****************************
    -->


<Cycle Name="uhh2::AnalysisModuleRunner" OutputDirectory="&SELdir;/" PostFix="" TargetLumi="&TargetLumi;" >
<!--<Cycle Name="uhh2::AnalysisModuleRunner" OutputDirectory="&SELdir;/" PostFix="" TargetLumi="&TargetLumi;" RunMode="PROOF" ProofServer="lite://" ProofWorkDir="&PROOFdir;/" ProofNodes="12">-->
    

  <InputData Lumi="1" NEventsMax="&NEVT;" Type="DATA" Version="DATA_B" Cacheable="&b_Cacheable;">
    <In FileName="&DATA_B;" Lumi="0.0"/>
    <InputTree Name="AnalysisTree" />
  </InputData>

  <InputData Lumi="1" NEventsMax="&NEVT;" Type="DATA" Version="DATA_C" Cacheable="&b_Cacheable;">
    <In FileName="&DATA_C;" Lumi="0.0"/>
    <InputTree Name="AnalysisTree" />
  </InputData>

  <InputData Lumi="1" NEventsMax="&NEVT;" Type="DATA" Version="DATA_D" Cacheable="&b_Cacheable;">
    <In FileName="&DATA_D;" Lumi="0.0"/>
    <InputTree Name="AnalysisTree" />
  </InputData>

  <InputData Lumi="1" NEventsMax="&NEVT;" Type="DATA" Version="DATA_E" Cacheable="&b_Cacheable;">
    <In FileName="&DATA_E;" Lumi="0.0"/>
    <InputTree Name="AnalysisTree" />
  </InputData>

  <InputData Lumi="1" NEventsMax="&NEVT;" Type="DATA" Version="DATA_F" Cacheable="&b_Cacheable;">
    <In FileName="&DATA_F;" Lumi="0.0"/>
    <InputTree Name="AnalysisTree" />
  </InputData>

               



 
        <UserConfig>
            <!-- define which collections to read from the input. Only specify what you need to save I/O time -->
            <Item Name="PrimaryVertexCollection" Value="offlineSlimmedPrimaryVertices" /> 
            <Item Name="ElectronCollection" Value="slimmedElectronsUSER" />
            <Item Name="MuonCollection" Value="slimmedMuonsUSER" /> 
            <!--<Item Name="TauCollection" Value="slimmedTaus" />-->
            <Item Name="JetCollection" Value="slimmedJets" />
            <Item Name="METName" Value="slimmedMETs" />
            <Item Name="GenJetCollection" Value="slimmedGenJets" /> 
	    <Item Name="GenParticleCollection" Value="GenParticles" />
	    <Item Name="lumi_file" Value="/nfs/dust/cms/user/reimersa/CMSSW_9_2_4/src/UHH2/common/data/Cert_294927-304507_13TeV_PromptReco_Collisions17_JSON.root"/>

            <Item Name="lumihists_lumi_per_bin" Value="300."/>
	    <Item Name="GenInfoName" Value="genInfo" />

            
            <!-- The list of available collections to read in corresponds to the pointer members in event. The 'Name's to use here in the configuration are:
            
                 PrimaryVertexCollection
                 ElectronCollection
                 MuonCollection
                 TauCollection
                 PhotonCollection
                 JetCollection
                 TopJetCollection
                 METName
                 
                 GenInfoName
                 GenJetCollection
                 GenTopJetCollection
                 GenParticleCollection
                 
                 Note that the last four are only read in for MC (InputData Type="MC"), so it is save to specify
                 them for real data, although those branches do not exist.
                 
                 As 'Value', always use the branch name (if in doubt, open the input file in ROOT).
                 
                 By default, triggers are read. To explicitly disable reading triggers, use:
                 <Item Name="readTrigger" Value="false" />
            -->
            
            <!-- the class name of the AnalysisModule subclasses to run: -->
            <Item Name="AnalysisModule" Value="LQToTopMuEarlyDataAnalysisModule" /> 
            
            <!-- tell AnalysisModuleRunner NOT to use the MC event weight from SFrame; rather let
                 MCLumiWeight (called via CommonModules) calculate the MC event weight. The MC
                 event weight assigned by MCLumiWeight is InputData.Lumi / Cycle.TargetLumi. -->
            <Item Name="use_sframe_weight" Value="false" />
            
            <!-- configuration for MCPileupReweight (run as part of CommonModules): -->
 
            <Item Name="pileup_directory" Value="/nfs/dust/cms/user/reimersa/CMSSW_9_2_4/src/UHH2/common/data/MyMCPileupHistogram.root" /> 
	    <Item Name="pileup_directory_data" Value="/nfs/dust/cms/user/reimersa/CMSSW_9_2_4/src/UHH2/common/data/MyDataPileupHistogram.root" />
	    <Item Name="pileup_directory_data_up" Value="/nfs/dust/cms/user/reimersa/CMSSW_9_2_4/src/UHH2/common/data/MyDataPileupHistogram_72383.root" />
	    <Item Name="pileup_directory_data_down" Value="/nfs/dust/cms/user/reimersa/CMSSW_9_2_4/src/UHH2/common/data/MyDataPileupHistogram_66017.root" />


	    <Item Name="Trigger"                Value="&Trigger;" />

	    <Item Name="Systematic_PU"          Value="central" />


            
            <!-- Some test configuration; see LQToTopMuModule.cxx for how to access it: -->
            <Item Name="TestKey" Value="TestKeyValue" />
        </UserConfig>
    </Cycle>
</JobConfiguration>
